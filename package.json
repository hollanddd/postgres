    "test": "vitest run",
    "dev": "vitest"
  },
  "devDependencies": {
    "@arethetypeswrong/cli": "^0.15.4",
    "prettier": "^3.3.3",
    "typescript": "^5.5.4",
    "vitest": "^2.0.5"
