{
  "name": "postgres",
  "version": "0.0.1",
  "description": "Postgres utility library",
  "keywords": [
    "postgres",
    "typescript"
  ],
  "homepage": "https://github.com/hollanddd/postgres",
  "bugs": {
    "url": "https://github.com/hollanddd/postgres/issues"
  },
  "author": "Darre Holland <mail@darrenholland.com> (https://darrenholland.com)",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hollanddd/postgres.git"
  },
  "files": [
    "dist"
  ],
  "type": "module",
  "license": "MIT",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
